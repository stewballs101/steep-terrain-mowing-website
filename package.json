{
  "name": "austere",
  "description": "Austere Risk Management",
  "version": "1.1.0",
  "author": "Brad Stewart <brad@austere.global>",
  "dependencies": {
    "gatsby": "^1.9.241",
    "gatsby-image": "^1.0.55",
    "gatsby-link": "^1.6.39",
    "gatsby-plugin-react-helmet": "^2.0.8",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "risk",
    "wilderness",
    "safety",
    "medical"
  ],
  "license": "MIT",
  "scripts": {
    "build:css": "tailwind build ./src/layouts/index.tailwind.css -c ./tailwind.config.js -o ./src/layouts/index.css",
    "build": "npm run build:css && purgecss -c ./purgecss.config.js -o ./src/layouts && gatsby build",
    "develop": "npm run build:css && gatsby develop",
    "format": "prettier --write src/**/*.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^1.11.1",
    "purgecss": "^1.0.1",
    "tailwindcss": "^0.6.1"
  }
}
